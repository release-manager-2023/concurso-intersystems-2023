{
  "name": "release-manager-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/release-manager-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/release-manager-e2e/cypress.json",
        "devServerTarget": "release-manager:serve",
        "tsConfig": "apps/release-manager-e2e/tsconfig.e2e.json"
      }
    },
    "configurations": {
      "production": {
        "devServerTarget": "release-manager:serve:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/release-manager-e2e/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "prefix": "app"
}
